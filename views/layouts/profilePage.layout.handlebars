<html lang="en" data-bs-theme="dark">

    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Trainer Home</title>
        <link rel="stylesheet" href="/public/css/userProfile.css " />
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
            integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN"
            crossorigin="anonymous" />
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css" />
        <link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css" rel="stylesheet" />
        <link rel="stylesheet" href="/public/css/header.css" />
        <link rel="stylesheet" href="/public/css/trainerSession.css" />
        <link rel="icon" type="image/jpeg" href="/public/imgs/gymMate_fevicon.jpeg" />
    </head>

    <body>
        <div class="container mt-5">
            <div class="container mt-5">
                <div class="row">
                    <div class="col-md-12">
                        <div id="content" class="content content-full-width">
                            <div class="profile">
                                <div class="profile-header">
                                    <div class="profile-header-cover"></div>
                                    <div class="profile-header-content">
                                        <div class="profile-header-img">
                                            <img src="https://bootdey.com/img/Content/avatar/avatar3.png" alt="" />
                                        </div>
                                        <div class="profile-header-info">
                                            <h4 class="m-t-10 m-b-5">{{trainer.trainerName}}</h4>
                                            <p class="m-b-10">Trainer</p>
                                            <a href="/trainer/profile" class="btn btn-sm btn-info mb-2">Profile</a>
                                        </div>
                                    </div>
                                    <ul class="profile-header-tab nav nav-tabs">
                                        <li class="nav-item"><a
                                                href="/user/trainerProfile/{{trainer.trainerName}}/posts"
                                                class="nav-link_ active show">POSTS</a></li>
                                        <li class="nav-item"><a
                                                href="/user/trainerProfile/{{trainer.trainerName}}/events"
                                                class="nav-link_">EVENTS</a></li>
                                        <li class="nav-item"><a
                                                href="/user/trainerProfile/{{trainer.trainerName}}/followers"
                                                class="nav-link_">FOLLOWERS</a></li>
                                        </li>
                                        <li class="nav-item"><a
                                                href="/user/trainerProfile/{{trainer.trainerName}}/following"
                                                target="__blank">FOLLOWING</a></li>
                                        <li class="nav-item"><a
                                                href="/user/trainerProfile/{{trainer.trainerName}}/sessions">SESSIONS</a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        {{{body}}}
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
            crossorigin="anonymous"></script>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
        <script src="/public/js/trainer/trainerHomeClient.js"></script>
        <script src="/public/js/helpersClient.js"></script>

    </body>

</html>
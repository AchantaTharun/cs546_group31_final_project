<div class="container">
  <aside class="left-panel">
    <h1 class="big-heading">{{title}}</h1>
    <a href="/api/v1/admin/homepage">HOMEPAGE</a>
    </aside>
    <div class="content">
      <h2 class="heading">REJECTED REQUEST LOGS</h2>
      <div class="window">
        <ol class="list">
          {{#each data}}
            <li><a href="/api/v1/admin/rejectedRequests/{{id}}">
            <div class="request-info">
              <div class="tile">
                <h3>{{requestType}}</h3><h4>ID: {{id}}</h4><h4>EMAIL: {{email}}</h4><h4>REASON: {{reason}}</h4>
              </div>
            </div>    
            </a></li>
          {{/each}}
        </ol>
      </div>
      <p class="error">{{error}}</p>
    </div>
  </div>


{{#if hasData}}
<form id="rej-display" novalidate>
    <label for="requestType">
        REQUEST TYPE :                                                 
        <input type="text" name="requestType" id="requestType" value="{{requestType}}" readonly>
    </label>
    <br>
    <label for="reason">
        REASON :                                                 
        <input type="text" name="reason" value="{{reason}}" id="reason" readonly>
    </label>
    <br>
    <label for="email">
        EMAIL :                                                 
        <input type="text" name="email" value="{{email}}" id="email" readonly>
    </label>
    <br>
    <label for="phone">
        PHONE :                                                 
        <input type="text" name="phone" value="{{phone}}" id="phone" readonly>
    </label>
    <br>
    <label for="street">
         STREET:                                                 
        <input type="text" name="street" value= "{{street}}" id="street" readonly>
    </label>
    <br>
    <label for="city">
        CITY :                                                 
        <input type="text" name="city" value="{{city}}" id="city" readonly>
    </label>
    <br>
    <label for="state">
        STATE :                                                 
        <input type="text" name="state" value="{{state}}" id="state" readonly>
    </label>
    <br>
    <label for="zip">
        ZIP :                                                 
        <input type="text" name="zip" value="{{zip}}" id="zip" readonly>
    </label>
</form>
{{/if}}

{{#unless hasData}}
<p id="error">{{error}}</p>
{{/unless}}

<a href="/api/v1/admin/homepage">BACK TO HOMEPAGE</a>